          "amazonPathToFile": {
            "type": "string",
            "nullable": true
          },
          "applicantComment": {
            "type": "string",
            "nullable": true
          },
          "expertComment": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "dateTime": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CVTag": {
        "enum": [
          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "Specialization": {
        "enum": [
          0,
          1
