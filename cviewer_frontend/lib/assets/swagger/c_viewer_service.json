            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CV": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "peopleCreatedId": {
            "type": "integer",
            "format": "int32"
          },
          "specialization": {
            "$ref": "#/components/schemas/Specialization"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "rating": {
            "type": "number",
            "format": "double"
          },
          "tags": {
            "type": "array",
