          "description": {
            "type": "string",
            "nullable": true
          },
          "tags": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CVTag"
            },
            "nullable": true
          },
          "dateCreation": {
            "type": "string",
            "format": "date-time"
          },
          "goodCv": {
            "type": "boolean"
          },
          "openToReview": {
            "type": "boolean"
          },
          "urlFileForDownload": {
            "type": "string",
            "nullable": true
          },
          "pinnedFileName": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CVDraftParameter": {
        "required": [
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          },
          "fileName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CVHistory": {
        "required": [
          "cvId",
          "dateTime",
          "id"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "cvId": {
            "type": "integer",
            "format": "int32"
          },
          "dateTime": {
            "type": "string",
            "format": "date-time"
          },
          "authorId": {
            "type": "integer",
            "format": "int32"
          },
          "fileName": {
            "type": "string",
            "nullable": true
          },
          "amazonPathToFile": {
            "type": "string",
            "nullable": true
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CVHistoryParameter": {
        "required": [
          "authorId",
          "cvId"
        ],
        "type": "object",
        "properties": {
          "cvId": {
            "type": "integer",
            "format": "int32"
          },
          "authorId": {
            "type": "integer",
            "format": "int32"
          },
          "fileName": {
            "type": "string",
            "nullable": true
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CVStatusType": {
        "enum": [
          "Draft",
          "SentToReview",
          "TakenToReview",
          "NeedFix",
          "Reviewed",
          "Finished"
        ],
        "type": "string"
      },
