          "peopleCreatedId",
          "statusId",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "statusId": {
            "$ref": "#/components/schemas/CVStatusType"
          },
          "title": {
            "type": "string"
          },
          "peopleCreatedId": {
            "type": "integer",
            "format": "int32"
          },
          "expertIds": {
            "type": "array",
